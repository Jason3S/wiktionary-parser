{
  "name": "wiktionary-parser",
  "version": "1.0.0",
  "description": "Using Javascript to parse Wiktionary Files -- A work in progress",
  "main": "main.js",
  "scripts": {
    "mocha": "./node_modules/.bin/mocha",
    "istanbul": "./node_modules/.bin/istanbul",
    "test": "npm run mocha -- --recursive build/test",
    "test:debug": "npm run mocha -- --recursive --debug-brk build/test",
    "test:w": "npm run mocha -- -w --recursive build/test",
    "coverage": "npm run istanbul cover ./node_modules/.bin/_mocha -- -- --recursive build/test",
    "tsc": "./node_modules/.bin/tsc",
    "tsc:w": "./node_modules/.bin/tsc -w",
    "webpack": "./node_modules/.bin/webpack",
    "webpack-dev-server": "./node_modules/.bin/webpack-dev-server --port 8088 --history-api-fallback --content-base . --inline",
    "build": "npm run tsc; npm run webpack",
    "start": "concurrently 'npm run tsc:w' 'npm run webpack-dev-server'",
    "deploy": "npm run tsc; npm run webpack",
    "typings": "./node_modules/.bin/typings install",
    "postinstall": "npm run typings",
    "tslint": "find . \\( -path './visapp*.ts*' -or -path './test*.ts*' \\) -exec ./node_modules/.bin/tslint {} \\;",
    "build:jison": "node ./build/jison/mygenerator.js ./build/jison ./jison/wiki-parser.jison ./jison/wiki-template.jison ./jison/wiktionary.jison",
    "build:wiki-parser": "node ./build/jison/mygenerator.js ./build/jison ./jison/wiki-parser.jison"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jason3S/wiktionary-parser.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jason3S/wiktionary-parser/issues"
  },
  "homepage": "https://github.com/Jason3S/wiktionary-parser#readme",
  "dependencies": {
    "async": "^1.5.0",
    "bluebird": "^3.1.1",
    "classnames": "^2.2.3",
    "detect-node": "^2.0.3",
    "es6-promise": "^3.0.2",
    "express": "^4.13.3",
    "history": "^2.0.1",
    "html-entities": "^1.2.0",
    "immutable": "^3.7.6",
    "install": "^0.4.1",
    "isomorphic-fetch": "^2.2.1",
    "jQuery": "^1.7.4",
    "jison": "^0.4.17",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "jsonpath": "^0.2.2",
    "lodash": "^3.10.1",
    "npm": "^3.8.8",
    "prettyjson": "^1.1.3",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-rx": "^0.5.0",
    "redux-simple-router": "^2.0.4",
    "redux-thunk": "^2.0.1",
    "request": "^2.67.0",
    "request-promise": "^1.0.2",
    "rx": "^4.1.0",
    "rx-dom": "^7.0.3",
    "tsmerge": "^1.0.1",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "JSONSelect": "^0.4.0",
    "chai": "^3.5.0",
    "concurrent": "^0.3.2",
    "concurrently": "^2.0.0",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.2.0",
    "html-webpack-plugin": "^1.7.0",
    "istanbul": "^0.4.3",
    "js-beautify": "^1.6.2",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "source-map-loader": "^0.1.5",
    "tslint": "^3.8.1",
    "typescript": "^1.8.10",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
